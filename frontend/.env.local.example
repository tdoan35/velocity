# Local Development Environment Variables
# Copy this file to .env.local and fill in your actual values
# DO NOT COMMIT .env.local to version control

# Application
VITE_APP_NAME=Velocity
VITE_APP_VERSION=1.0.0
VITE_PORT=5173

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false

# Third-party Services
VITE_MONACO_CDN_URL=https://cdn.jsdelivr.net/npm/monaco-editor@0.45.0
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=

# Velocity's Supabase Configuration
# Get these from your Velocity Supabase project settings
VITE_SUPABASE_URL=https://your-velocity-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-velocity-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-velocity-service-role-key

# Supabase OAuth Configuration
# These allow users to connect their own Supabase projects to Velocity
# Register at: https://supabase.com/partners/integrations
VITE_SUPABASE_OAUTH_ENABLED=true
VITE_SUPABASE_MANAGEMENT_API_URL=https://api.supabase.com/v1
SUPABASE_OAUTH_CLIENT_ID=your-oauth-client-id
SUPABASE_OAUTH_CLIENT_SECRET=your-oauth-client-secret
SUPABASE_OAUTH_REDIRECT_URI=http://localhost:5173/auth/supabase/callback

# Optional: Personal access token for development/testing
SUPABASE_ACCESS_TOKEN=your-personal-access-token

# Container Preview Configuration (Fly.io)
VITE_ORCHESTRATOR_URL=https://velocity-orchestrator.fly.dev/

# AI Configuration (optional)
VITE_ANTHROPIC_API_KEY=
VITE_OPENAI_API_KEY=
VITE_ENABLE_AI_STREAMING=true

# Build Configuration
VITE_SOURCE_MAP=true
VITE_BUNDLE_ANALYZE=false