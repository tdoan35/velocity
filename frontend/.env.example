# Application
VITE_APP_NAME=Velocity
VITE_APP_VERSION=1.0.0
VITE_PORT=5173

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false

# Third-party Services
VITE_MONACO_CDN_URL=https://cdn.jsdelivr.net/npm/monaco-editor@0.45.0
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=

# Velocity's Supabase Configuration (for user auth and app data)
VITE_SUPABASE_URL=your_velocity_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_velocity_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_velocity_service_role_key

# User Auth OAuth Redirect URLs (configure in your Supabase Dashboard):
# - Google OAuth: http://localhost:5173/auth/callback
# - GitHub OAuth: http://localhost:5173/auth/callback

# Credential Security Configuration
# IMPORTANT: Generate a strong, unique key for production using: openssl rand -hex 32
VITE_CREDENTIAL_ENCRYPTION_KEY=your_32_byte_hex_encryption_key_here

# Supabase Direct Connection Configuration (for users to connect their own Supabase projects)
# Users provide their own Supabase project URL and anon key directly
# No OAuth flow required - users maintain full control of their Supabase projects
VITE_SUPABASE_CONNECTION_ENABLED=true
VITE_SUPABASE_CONNECTION_RATE_LIMIT=10
VITE_SUPABASE_CONNECTION_TIMEOUT=10000

# Container Preview Configuration (Fly.io)
VITE_ORCHESTRATOR_URL=https://your-orchestrator-app.fly.dev

# Build Configuration
VITE_SOURCE_MAP=true
VITE_BUNDLE_ANALYZE=false

# Production OAuth Redirect URI (update when deploying):
# SUPABASE_OAUTH_REDIRECT_URI=https://your-production-domain.com/auth/supabase/callback